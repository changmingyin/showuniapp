{
  "name": "uniapp",
  "version": "0.1.0-dev.0",
  "description": "uniapp 框架基础模块",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "scripts": {
    "start": "tsc -v && (tsc -w & tsc -p tsconfig-esc.json -w)",
    "build-base": "tsc -d --declarationDir ./dist/typings/",
    "build-esc": "tsc -p tsconfig-esc.json -d --declarationDir ./dist/typings/extra-subcommands",
    "build": "rimraf dist && tsc -v && (npm run build-base & npm run build-esc)",
    "deploy-dev": "npm run build && npm publish --tag dev",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/maolion/uniapp.git"
  },
  "keywords": [
    "uniapp"
  ],
  "author": "maolion.j@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/maolion/uniapp/issues"
  },
  "homepage": "https://github.com/maolion/uniapp#readme",
  "devDependencies": {
    "@types/immutable": "^3.8.5",
    "@types/lodash": "^4.14.55",
    "@types/node": "^7.0.8",
    "@types/react": "^15.0.16",
    "@types/redux": "^3.6.31",
    "rimraf": "^2.6.1",
    "typescript": "^2.2.1"
  },
  "dependencies": {
    "events": "^1.1.1",
    "immutable": "^3.8.1",
    "lodash": "^4.17.4",
    "mockjs": "^1.0.1-beta3",
    "react": "^15.3.2",
    "villa": "^0.2.11"
  }
}
